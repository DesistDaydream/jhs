<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" android:padding="6dp" android:layout_width="match_parent" android:layout_height="wrap_content">
    <include android:id="@+id/include_time" layout="@layout/ykfsdk_ykf_chatitem_time"/>
    <RelativeLayout android:background="@drawable/ykfsdk_kf_coner_bg" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="16dp" android:layout_marginRight="16dp" android:layout_below="@+id/include_time">
        <TextView android:textSize="16sp" android:textColor="@color/ykfsdk_grey" android:id="@+id/tv" android:padding="4dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/ykfsdk_investigate"/>
        <View android:id="@+id/vi" android:background="@color/ykfsdk_grey" android:layout_width="match_parent" android:layout_height="1dp" android:layout_margin="2dp" android:layout_below="@+id/tv"/>
        <LinearLayout android:orientation="vertical" android:id="@+id/chat_investigate_ll" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_below="@+id/vi"/>
    </RelativeLayout>
</RelativeLayout>
